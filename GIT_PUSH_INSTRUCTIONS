cd "e:\E-Project\Crystal-Products"
git status
# if there are local changes you want to keep:
git add -A
git commit -m "chore: save local changes (no functional changes)"
# check existing remotes
git remote -v
# if no remote, add one (replace with your repo URL)
# git remote add origin git@github.com:YOUR_USERNAME/YOUR_REPO.git
# or (HTTPS)
# git remote add origin https://github.com/YOUR_USERNAME/YOUR_REPO.git
# push current branch and set upstream
git rev-parse --abbrev-ref HEAD   # shows current branch name
git push -u origin HEAD
# verify remote status
git log --oneline -n 5
git remote show origin
